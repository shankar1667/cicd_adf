{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "shankardev"
		},
		"ds_Nplus rec_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ds_Nplus rec_sql'"
		},
		"ds_ncols_n_plus rec_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ds_ncols_n_plus rec'"
		},
		"ls_blob_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_blob'"
		},
		"ls_out_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_out'"
		},
		"ds_Nplus rec_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "shankar-server.database.windows.net"
		},
		"ds_Nplus rec_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "shankar_db"
		},
		"ds_Nplus rec_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"ds_ncols_n_plus rec_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shankarblob2725.dfs.core.windows.net/"
		},
		"ls_blob_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shankarblob2725.dfs.core.windows.net/"
		},
		"ls_out_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shankaradlsgen22725.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "shankardev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ds_Nplus rec_sql_password": {
						"value": "[parameters('ds_Nplus rec_sql_password')]"
					},
					"ds_ncols_n_plus rec_accountKey": {
						"value": "[parameters('ds_ncols_n_plus rec_accountKey')]"
					},
					"ls_blob_accountKey": {
						"value": "[parameters('ls_blob_accountKey')]"
					},
					"ls_out_accountKey": {
						"value": "[parameters('ls_out_accountKey')]"
					},
					"ds_Nplus rec_sql_properties_typeProperties_server": {
						"value": "[parameters('ds_Nplus rec_sql_properties_typeProperties_server')]"
					},
					"ds_Nplus rec_sql_properties_typeProperties_database": {
						"value": "[parameters('ds_Nplus rec_sql_properties_typeProperties_database')]"
					},
					"ds_Nplus rec_sql_properties_typeProperties_userName": {
						"value": "[parameters('ds_Nplus rec_sql_properties_typeProperties_userName')]"
					},
					"ds_ncols_n_plus rec_properties_typeProperties_url": {
						"value": "[parameters('ds_ncols_n_plus rec_properties_typeProperties_url')]"
					},
					"ls_blob_properties_typeProperties_url": {
						"value": "[parameters('ls_blob_properties_typeProperties_url')]"
					},
					"ls_out_properties_typeProperties_url": {
						"value": "[parameters('ls_out_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}